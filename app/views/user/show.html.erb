<div class="logo-dashboard-page">
  <%=link_to(image_tag("TrainTracksLogo2.png", height: "50%", width: "50%"), root_path) %>
</div>
<div class="dashboard-logout">
  <%= link_to "LOGOUT", logout_path, method: :delete, class: 'btn btn-default btn-lg logout-spotify' %>
</div>
<div class="dashboard-user">
  <div class="dashboard-picture">
    <%= image_tag current_user.image_url, height: "40%", width: "40%", class: "img-circle" if current_user %>
  </div>
  <p><%= "#{current_user.name}" %> </p>
</div>
<div class="dashboard-options">
  <div class="dashboard-new-playlist">
    hello
  </div>
  <div class="dashboard-old-playlists">
    hi
  </div>
  <div class="dashboard-random-playlist">
    how ya dern
  </div>
</div>
